package practiseIO;

public class Demo7_Wrap {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Superman sm = new Superman(new Man());
		sm.skill();
	}
	interface Skills{
		public void skill();
	}
	static class Man implements Skills {

		@Override
		public void skill() {
			// TODO Auto-generated method stub
			System.out.println("i can walk");
		}
		
	}
	static class Superman implements Skills{
		private Man m;
		
		public Superman(Man m) {
			this.m = m;
		}
		@Override
		public void skill() {
			// TODO Auto-generated method stub
			m.skill();
			System.out.println("i can fly");
		}
		
	}
}
