package practiseIO;

public class Demo7_Wrap {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	interface Skills{
		public void skill();
	}
	public class man implements Skills {

		@Override
		public void skill() {
			// TODO Auto-generated method stub
			System.out.println("i can walk");
		}
		
	}
	public class superman implements Skills{
		private man m;
		
		public superman(man m) {
			this.m = m;
		}
		@Override
		public void skill() {
			// TODO Auto-generated method stub
			m.skill();
			System.out.println("i can fly");
		}
		
	}
}
