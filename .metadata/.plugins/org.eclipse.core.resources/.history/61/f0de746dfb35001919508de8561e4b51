package practise2;

public class Circle extends Shape {

	@Override
	public double l(double a, double b) {
		// TODO Auto-generated method stub
		double c=(a+b)*2;
		return 0;
	}

	@Override
	public double s(double a, double b) {
		// TODO Auto-generated method stub
		double c = a*b;
		return 0;
	}

}
